SELECT * FROM dogs;

SELECT sum(price) FROM dogs;

SELECT price FROM dogs
WHERE price >= 200;

SELECT * FROM dogs
ORDER BY price;

SELECT price FROM dogs
WHERE price < 250 AND price > 1;



